<head>
  <!-- Taboola Pixel Code -->
  <script type="text/javascript">
    window._tfa = window._tfa || [];
    window._tfa.push({ notify: 'event', name: 'page_view', id: 1463942 });
    !function (t, f, a, x) {
      if (!document.getElementById(x)) {
        t.async = 1; t.src = a; t.id = x; f.parentNode.insertBefore(t, f);
      }
    }(document.createElement('script'),
      document.getElementsByTagName('script')[0],
      '//cdn.taboola.com/libtrc/unip/1463942/tfa.js',
      'tb_tfa_script');
    {% if template contains 'product' %}
    _tfa.push({ notify: 'event', name: 'view_content', id: 1463942 });
    {% endif %}
  </script>
  <script src="//cdn.codeblackbelt.com/js/modules/frequently-bought-together/main.min.js?shop=pexmodo.myshopify.com" defer></script>
  <!-- End of Taboola Pixel Code -->
  
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-9WDFET9QS9"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-9WDFET9QS9');
</script>
<script>
	window.addEventListener('load',function()
	{
		if(window.location.href.includes('/products/')) 
		{
			gtag("event", "view_item", {
			    'send_to': 'G-9WDFET9QS9',
                items: [
                {
                  item_name: document.querySelector('[class="product_title entry-title"]').innerText,
                  currency: Shopify.currency.active,
                  price: document.querySelector('#price_ppr ins').innerText.replace(/[^0-9.]/g, '')
                }
                ]
            });
            document.querySelector('.single_add_to_cart_button').addEventListener('click',function()
	        {
	            gtag("event", "add_to_cart", {
	                'send_to': 'G-9WDFET9QS9',
                    items: [
                    {
                      item_name: document.querySelector('[class="product_title entry-title"]').innerText,
                      currency: Shopify.currency.active,
                      price: document.querySelector('#price_ppr ins').innerText.replace(/[^0-9.]/g, '')
                    }
                    ]
                });
	        })
		}
	})
</script>

