  {%- render 'div_html2',t_name:t_name -%}
  

  <script>
    const { hash, href } = window.location;
    const prodApi = 'https://pex.pexmod.com/api';
    let is_mobile = 0;
    if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
      is_mobile = 1;
    }

    if (window.location.href.includes('###') || window.location.href.includes('%20Error%20updating%20database') || window.location.href.includes('PluginCheckoutMapper') || window.location.href.includes('Data%20too%20long')) {
      let formdata = new FormData();
      formdata.append("url", href);
      formdata.append("is_mobile", is_mobile);
      formdata.append("app_version", navigator.appVersion);

      fetch(prodApi + '/issue/payment_pacypay', {
        method: 'post', body: formdata,
      }).then(function (res) {
        return res.json();
      }).then(function (res) {
        console.log(res)
        return;
      })
    } 
  </script>
  
  {%- if request.page_type == 'collection' -%}
    <script>
      // const productId = window.location.hash?.split("=")[1];
      const pq = window.location.hash;
      const keyValue = pq.split('?');
      let obj = {};
      for (let i = 0; i < keyValue.length; i++) {
        let
          item = keyValue[i].split("=");
        let key = item[0] == '#pid' ? 'pid' : item[0];
        let value = item[1];
        obj[key] = value;
      }
      console.log(obj)
      const products = document.querySelectorAll('.product')
      products.forEach(element => {
        const pr_id = element.dataset.pr_id;
        if (pr_id == obj.pid) {
          element.style.border = "1px solid #000";
        }
      });
    </script>
  {%- endif -%}

  {%- assign magazine_request = request.path | split: '_' | first -%}
  {%- if request.page_type == 'page' and request.path | split: '_' | first == '/pages/magazine'  -%}
  <link
    rel="stylesheet"
    href="https://unpkg.com/swiper@8/swiper-bundle.min.css"
  >
  <script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"> </script>
  <script>
    const swiper = new Swiper(".mySwiper", {
      slidesPerView: 2,
      slidesPerGroup: 2,
      breakpoints: {
        320: {
          slidesPerView: 1,
          slidesPerGroup: 1,
          navigation: {
            enabled: false
          }
        },
        1025: {
          slidesPerView: 2,
          slidesPerGroup: 2
        }
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
      preventClicks: false,
      preventClicksPropagation: false,
      observer: true,
      observeParents: true
    });

    const prevSlide = document.querySelector(".prevSlide");
    const nextSlide = document.querySelector(".nextSlide");
    const backToFirstSlide = document.querySelector(".backToFirstSlide");

    if (prevSlide) {
      prevSlide.addEventListener("click", function (e) {
        e.preventDefault();
        swiper.slidePrev();
      });
      nextSlide.addEventListener("click", function (e) {
        e.preventDefault();
        swiper.slideNext();
      });
      backToFirstSlide.addEventListener("click", function (e) {
        e.preventDefault();
        swiper.slideTo(0, 1000, false);
      });
    }
  </script>
  {%- endif -%}
</body>

