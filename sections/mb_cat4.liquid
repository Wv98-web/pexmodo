{%- liquid
 assign limit = 5
 assign show_pr = false
 assign img_size = '40x' 
 assign only_icon = 'only_icon_' | append:settings.only_icon -%}

<ul id="menu_mb_cat" class="nt_mb_menu menu_mb_cat4">
   {%- if section.blocks.size > 0 -%}
      {%- assign arr_blocks_img = section.blocks | where: "type", 'image' -%}
      {%- if arr_blocks_img.size > 0 -%}
      <div class="menu_mb_cat4_title">
         <div style="background-color: transparent;border-bottom: none;">
            <span class="nav_link_txt flex al_center h3">Pexmod</span>
         </div>
      </div>
      {%- for block in arr_blocks_img %}
         {% assign bk_stts = block.settings %}
         {% assign image = bk_stts.image -%}
         <li id="item_{{block.id}}" class="menu-item item-level-0 menu-item-has-children nt_opended" {{ block.shopify_attributes }}>
            <div class="cat_space_item hoverz_{{bk_stts.hoverz}} col-lg-{{bk_stts.col_pr}} col-md-{{bk_stts.col_pr_tb}} col-{{bk_stts.col_pr_mb}} pr_animated done bg_content_{{bk_stts.bg_content_bl}} border_bl_{{bk_stts.border_bl}} br_style_{{bk_stts.br_style}}" id="bk_{{block.id}}">
               <div class="banner_hzoom nt_promotion oh pr">
                  {%- if image != blank %}
                     {% assign image_mb = bk_stts.image_mb | default: image -%}
                     <div class="dek_img_slide nt_bg_lz pr_lazy_img lazyloadt4s item__position mb__10" data-bgset="{{ image | img_url: '1x1' }}" data-ratio="{{image.aspect_ratio}}" data-sizes="auto" data-parent-fit="width" style="padding-top:{{ 1 | divided_by: image.aspect_ratio | times: 100}}%;"></div>
                     <div class="mb_img_slide nt_bg_lz pr_lazy_img lazyloadt4s item__position mb__10" data-bgset="{{ image_mb | img_url: '1x1' }}" data-ratio="{{image_mb.aspect_ratio}}" data-sizes="auto" data-parent-fit="width" style="padding-top:{{ 1 | divided_by: image_mb.aspect_ratio | times: 100}}%;"></div>
                  {%- else -%}
                     {%- capture cl -%}{% cycle 'a8dacc', 'f76b6a' %}{%- endcapture -%}
                     <div class="nt_bg_lz pr_lazy_img lazyloadt4s item__position {{bk_stts.img_align}}" data-bg="https://placehold.jp/50/{{cl}}/{{cl}}/530x600.png?text=1" style="padding-top:52.63157894736842%"></div>
                  {%- endif -%}
                  <a href="{{ bk_stts.url }}" target="{{bk_stts.open_link}}" class="pa t__0 l__0 r__0 b__0{% if bk_stts.url == blank %} pe_none{% endif %}"></a>
               </div>
            </div>
         </li>
      {%- endfor -%}

      {%- else -%}
      {%- for block in section.blocks -%}
         {%- assign block_stts = block.settings -%}
         {%- assign collection = collections[block_stts.cat] -%}
         {%- assign tt_current = block_stts.title | default:collection.title -%}
         {%- assign url_current = block_stts.url | default:collection.url -%}
            {%- case block.type -%}
               {%- when 'menu2' -%}
                  {%- if linklists[block_stts.menu].links.size > 0 -%}
                     <li id="item_{{block.id}}" class="menu-item item-level-0 menu-item-has-children {{only_icon}} nt_opended" {{ block.shopify_attributes }}>
                        <a href="{{block_stts.url}}" target="{{block_stts.open_link}}" style="background-color: transparent;border-bottom: none;">
                           <span class="nav_link_txt flex al_center h3">
                              {% if block_stts.icon != blank %}
                              <i class="las la-{{block_stts.icon | strip }} mr__10 fs__20"></i>
                              {% endif %}

                              {{block_stts.title}}

                              {% if block_stts.lb != blank %}
                              <span class="lbc_nav_mb ml__5" style="background-color: {{block_stts.lb_cl | default:'#01bad4'}}">{{block_stts.lb}}</span>
                              {% endif %}
                           </span>
                        </a>
                        <ul class="sub-menu">
                           <div class="flex fl_wrap mt__20 mb__20">
                              {%- for link in linklists[block_stts.menu].links -%}
                                 {%- assign arrlt = link.title | split: '[' -%}
                                 {%- assign collection = link.object -%}
                                 {%- assign first_product = collection.products[0] -%}
                                 {%- assign first_product_img = first_product.images[0] -%}
                                 <div class="type_mn_pr menu-item sub-column-item col-4 equal_nt nt_cover position_8 ratio_adapt hoverz_false cat_design_5">
                                    <a href="{{link.url}}">
                                       <div class="cat_grid_item cat_space_item">
                                          <div class="cat_grid_item__content pr oh">
                                             <div class="db cat_grid_item__link ">
                                                <div class="cat_grid_item__overlay item__position nt_bg_lz lazyloadt4s" data-bgset="{{ first_product_img | img_url: '1x1' }}" data-ratio="{{first_product_img.aspect_ratio}}" data-sizes="auto" data-parent-fit="width" style="padding-top:110%;margin-bottom: 0;"></div>
                                             </div>
                                             <div class="cat_grid_item__title tc">
                                                {%- render 'lb_inc_mb2',arrlt:arrlt -%}
                                             </div>
                                          </div>
                                       </div>
                                    </a>
                                 </div>

                              {%- endfor -%}
                           </div>
                        </ul>
                     </li>
                  {%- else -%}
                     <li id="item_{{block.id}}" class="menu-item item-level-0"><a href="{% if block_stts.url contains '#homet4' %}{{ root_url }}{% else %}{{block_stts.url}}{% endif %}" target="{{block_stts.open_link}}">{% if block_stts.icon != blank %}<i class="las la-{{block_stts.icon | strip }} mr__10 fs__20"></i>{% endif %}{{block_stts.title }}{% if block_stts.lb != blank %}<span class="lbc_nav_mb ml__5" style="background-color: {{block_stts.lb_cl | default:'#01bad4'}}">{{block_stts.lb}}</span>{% endif %}</a></li>
                  {%- endif -%}
              {%- when 'cat' -%}
                  {%- if linklists[block_stts.menu].links.size > 0 -%}
                      <li id="item_{{block.id}}" class="menu-item item-level-0 menu-item-has-children {{only_icon}}" {{ block.shopify_attributes }}>
                        <a href="{{url_current}}"><span class="nav_link_txt flex al_center">

                         {%- if block_stts.icons_op == '2' and block_stts.icon != blank %}<i class="las la-{{block_stts.icon | strip }} mr__10 fs__20"></i>
                         {%- elsif block_stts.icons_op == '1' and block_stts.image != blank %}<i class="dib mr__10"><img class="img_catk_mb" src="{{block_stts.image | img_url:img_size }}" alt="{{block_stts.image.alt}}"></i>{% endif -%}

                         {{tt_current}}{% if block_stts.lb != blank %}<span class="lbc_nav_mb ml__5" style="background-color: {{block_stts.lb_cl}}">{{block_stts.lb}}</span>{% endif %}</span><span class="nav_link_icon ml__5"></span></a>
                           <ul class="sub-menu">
                              {%- for link in linklists[block_stts.menu].links -%}

                                 {%- assign arrlt = link.title | split: '[' -%}{%- render 'lb_inc',arrlt:arrlt -%}
                                 {%- if link.links != blank -%}
                                    <li class="menu-item item-level-1 menu-item-has-children {{only_icon}}{% if link.active %} current_item{% endif %}">
                                       <a href="{{link.url}}"><span class="nav_link_txt flex al_center">{%- render 'lb_inc_mb',arrlt:arrlt -%}</span><span class="nav_link_icon ml__5"></span></a>
                                       <ul class="sub-sub-menu">
                                          {%- for child_link in link.links -%}

                                             {%- assign arrlt = child_link.title | split: '[' -%}
                                             {%- if child_link.links != blank -%}
                                                <li id="item_{{block.id}}" class="menu-item item-level-2 menu-item-has-children {{only_icon}}{% if child_link.active %} current_item{% endif %}">
                                                   <a href="{{child_link.url}}"><span class="nav_link_txt flex al_center">{%- render 'lb_inc_mb',arrlt:arrlt -%}</span><span class="nav_link_icon ml__5"></span></a>
                                                   <ul class="sub-sub-sub-menu">

                                                      {%- for grandchild_link in child_link.links -%}
                                                      {%- assign arrlt = grandchild_link.title | split: '[' -%}
                                                      <li class="menu-item item-level-3{% if grandchild_link.active %} current_item{% endif %}"><a href="{{grandchild_link.url}}">{%- render 'lb_inc_mb',arrlt:arrlt -%}</a></li>
                                                      {%- endfor -%}

                                                   </ul>
                                                </li>
                                             {%- else -%}
                                                 <li class="menu-item item-level-2{% if child_link.active %} current_item{% endif %}"><a href="{{child_link.url}}">{%- render 'lb_inc_mb',arrlt:arrlt -%}</a></li>
                                             {%- endif -%}

                                          {%- endfor -%}
                                       </ul>
                                    </li>
                                 {%- else -%}
                                    <li class="menu-item item-level-1{% if link.active %} current_item{% endif %}"><a href="{{link.url}}">{%- render 'lb_inc_mb',arrlt:arrlt -%}</a></li>
                                 {%- endif -%}

                              {%- endfor -%}
                           </ul>
                     </li>
                  {%- else -%}
                      <li id="item_{{block.id}}" class="menu-item item-level-0"><a href="{{url_current}}">

                       {%- if block_stts.icons_op == '2' and block_stts.icon != blank %}<i class="las la-{{block_stts.icon | strip }} mr__10 fs__20"></i>
                       {%- elsif block_stts.icons_op == '1' and block_stts.image != blank %}<i class="dib mr__10"><img class="img_catk_mb" src="{{block_stts.image | img_url:img_size }}" alt="{{block_stts.image.alt}}"></i>{% endif -%}

                       {{tt_current}}{% if block_stts.lb != blank %}<span class="lbc_nav_mb ml__5" style="background-color: {{block_stts.lb_cl}}">{{block_stts.lb}}</span>{% endif %}</a></li>
                  {%- endif -%}

              {%- when 'cat_list' -%}{% if linklists[block_stts.menu].links.size == 0 %}{% continue %}{% endif -%}
               {%- assign block_id = block.id -%}
               {%- for link in linklists[block_stts.menu].links -%}
                  {%- assign arrlt = link.title | split: '[' -%}

                  {%- if link.links != blank -%}
                     <li id="item_{{block_id}}{{link.level}}{{forloop.index}}" class="menu-item item-level-0 menu-item-has-children {{only_icon}}{% if link.active %} current_item{% endif %}">
                        <a href="{{link.url}}"><span class="nav_link_txt flex al_center">{%- render 'lb_inc_mb',arrlt:arrlt -%}</span><span class="nav_link_icon ml__5"></span></a>
                        <ul class="sub-menu">
                           {%- for sub_link in link.links -%}

                              {%- assign arrlt = sub_link.title | split: '[' -%}
                              {%- if sub_link.links != blank -%}
                                 <li class="menu-item item-level-1 menu-item-has-children {{only_icon}}{% if sub_link.active %} current_item{% endif %}">
                                    <a href="{{sub_link.url}}"><span class="nav_link_txt flex al_center">{%- render 'lb_inc_mb',arrlt:arrlt -%}</span><span class="nav_link_icon ml__5"></span></a>
                                    <ul class="sub-sub-menu">
                                       {%- for sub_sub_link in sub_link.links -%}

                                          {%- assign arrlt = sub_sub_link.title | split: '[' -%}
                                          {%- if sub_sub_link.links != blank -%}
                                             <li id="item_{{block.id}}" class="menu-item item-level-2 menu-item-has-children {{only_icon}}{% if sub_sub_link.active %} current_item{% endif %}">
                                                <a href="{{sub_sub_link.url}}"><span class="nav_link_txt flex al_center">{%- render 'lb_inc_mb',arrlt:arrlt -%}</span><span class="nav_link_icon ml__5"></span></a>
                                                <ul class="sub-sub-sub-menu">

                                                   {%- for grandchild_link in sub_sub_link.links -%}
                                                   {%- assign arrlt = grandchild_link.title | split: '[' -%}
                                                   <li class="menu-item item-level-3{% if grandchild_link.active %} current_item{% endif %}"><a href="{{grandchild_link.url}}">{%- render 'lb_inc_mb',arrlt:arrlt -%}</a></li>
                                                   {%- endfor -%}

                                                </ul>
                                             </li>
                                          {%- else -%}
                                              <li class="menu-item item-level-2{% if sub_sub_link.active %} current_item{% endif %}"><a href="{{sub_sub_link.url}}">{%- render 'lb_inc_mb',arrlt:arrlt -%}</a></li>
                                          {%- endif -%}

                                       {%- endfor -%}
                                    </ul>
                                 </li>
                              {%- else -%}
                                 <li class="menu-item item-level-1{% if sub_link.active %} current_item{% endif %}"><a href="{{sub_link.url}}">{%- render 'lb_inc_mb',arrlt:arrlt -%}</a></li>
                              {%- endif -%}
                           {%- endfor -%}
                        </ul>
                     </li>

                  {%- else -%}
                  <li id="item_{{block_id}}{{link.level}}{{forloop.index}}" class="menu-item item-level-0{% if link.active %} current_item{% endif %}"><a href="{{link.url}}">{%- render 'lb_inc_mb',arrlt:arrlt -%}</a></li>
                  {%- endif -%}

               {%- endfor -%}

              {%- else -%}
            
               {%- capture get_sub_cat -%}
                      {%- for i in (1..25) -%}
                      {%- assign cat = 'cat' | append:i -%}
                      {%- assign url = 'url' | append:i -%}
                      {%- assign image = 'image' | append:i -%}
                      {%- assign cat_id = block_stts[cat] -%}
                      {%- assign url_id = block_stts[url] -%}
                      {%- assign image_id = block_stts[image] -%}
                      {%- assign collec = collections[cat_id] -%}

                          {%- if collec == blank and url_id == blank %}{% continue %}{% endif %}{% assign image = image_id | default:collec.image -%}
                           <div class="cat_grid_item cat_space_item">
                            <div class="cat_grid_item__content pr oh">
                               <a href="{{ url_id|default:collec.url }}" class="db cat_grid_item__link">
                                {%- if image != blank -%}
                                <div class="cat_grid_item__overlay item__position nt_bg_lz lazyloadt4s" data-bgset="{{ image | img_url: '1x1' }}" data-ratio="{{image.aspect_ratio}}" data-sizes="auto" data-parent-fit="width" style="padding-top:{{ 1 | divided_by: image.aspect_ratio | times: 100}}%;"></div>
                                {%- else -%}
                                <div class="cat_grid_item__overlay item__position nt_bg_lz" style="padding-top:100%;"></div>
                                {%- endif -%}
                               </a>
                               <div class="cat_grid_item__wrapper pe_none"><div class="cat_grid_item__title h3">{{ collec.title }}</div></div>
                            </div>
                           </div>
                         
                      {%- endfor -%}
               {%- endcapture -%}

               {%- if get_sub_cat != blank -%}
                   <li id="item_{{block.id}}" class="menu-item menu-item-cat item-level-0 menu-item-has-children {{only_icon}}" {{ block.shopify_attributes }}>
                     <a href="{{url_current}}"><span class="nav_link_txt flex al_center">

                      {%- if block_stts.icons_op == '2' and block_stts.icon != blank %}<i class="las la-{{block_stts.icon | strip }} mr__10 fs__20"></i>
                      {%- elsif block_stts.icons_op == '1' and block_stts.image != blank %}<i class="dib mr__10"><img class="img_catk_mb" src="{{block_stts.image | img_url:img_size }}" alt="{{block_stts.image.alt}}"></i>{% endif -%}

                      {{tt_current}}{% if block_stts.lb != blank %}<span class="lbc_nav_mb ml__5" style="background-color: {{block_stts.lb_cl}}">{{block_stts.lb}}</span>{% endif %}</span><span class="nav_link_icon ml__5"></span></a>
                      <ul class="sub-menu equal_nt cat_design_2">{{get_sub_cat}}</ul>
                  </li>
               {%- else -%}
                   <li id="item_{{block.id}}" class="menu-item item-level-0"><a href="{{url_current}}">

                    {%- if block_stts.icons_op == '2' and block_stts.icon != blank %}<i class="las la-{{block_stts.icon | strip }} mr__10 fs__20"></i>
                    {%- elsif block_stts.icons_op == '1' and block_stts.image != blank %}<i class="dib mr__10"><img class="img_catk_mb" src="{{block_stts.image | img_url:img_size }}" alt="{{block_stts.image.alt}}"></i>{% endif -%}

                    {{tt_current}}{% if block_stts.lb != blank %}<span class="lbc_nav_mb ml__5" style="background-color: {{block_stts.lb_cl}}">{{block_stts.lb}}</span>{% endif %}</a></li>
               {%- endif -%}
            {%- endcase -%}
      {%- endfor -%}
      {%- endif -%}

   {%- else -%}
   <li><a href="/admin/themes/">Create your mobile categories menu sidebar and config from Section > Mobile Categories</a></li>
   {%- endif -%}
</ul>

{%- schema -%}
{
   "name": "Pexmod",
   /*"class": "mb_nav_tab",*/
   "max_blocks": 30,
   "blocks": [
      {
      "type": "image",
      "name": "Image (Parent)",
      "settings": [
         {
            "type": "text",
            "id": "title",
            "label": "Heading",
            "default": "Heading"
         },
         {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
         },
         {
            "type": "image_picker",
            "id": "image_mb",
            "label": "Mobile image (optional)"
         },
         {
            "type": "url",
            "id": "url",
            "label": "Link image",
            "info": "The whole image becomes clickable."
         },
         {
            "type": "select",
            "id": "open_link",
            "options": [
               {
                  "value": "_self",
                  "label": "Current window (_self)"
               },
               {
                  "value": "_blank",
                  "label": "New window (_blank)"
               }
            ],
            "label": "Open link in",
            "default": "_blank"
         },
         {
            "type": "checkbox",
            "id": "hoverz",
            "label": "Enable zoom image on hover?",
            "default": true
         },
         {
            "type": "select",
            "id": "col_pr",
            "label": "Banner width (desktop)",
            "default": "6",
            "options": [
               {
                  "value": "12",
                  "label": "100%"
               },
               {
                  "value": "8",
                  "label": "66,6%"
               },
               {
                  "value": "7",
                  "label": "58,3%"
               },
               {
                  "value": "6",
                  "label": "50%"
               },
               {
                  "value": "5",
                  "label": "41,6%"
               },
               {
                  "value": "4",
                  "label": "33,3%"
               },
               {
                  "value": "3",
                  "label": "25%"
               },
               {
                  "value": "15",
                  "label": "20%"
               },
               {
                  "value": "2",
                  "label": "16,6%"
               }
            ]
         },
         {
            "type": "select",
            "id": "col_pr_tb",
            "label": "Banner width (tablet)",
            "default": "6",
            "options": [
               {
                  "value": "12",
                  "label": "100%"
               },
               {
                  "value": "8",
                  "label": "66,6%"
               },
               {
                  "value": "7",
                  "label": "58,3%"
               },
               {
                  "value": "6",
                  "label": "50%"
               },
               {
                  "value": "5",
                  "label": "41,6%"
               },
               {
                  "value": "4",
                  "label": "33,3%"
               },
               {
                  "value": "3",
                  "label": "25%"
               },
               {
                  "value": "15",
                  "label": "20%"
               },
               {
                  "value": "2",
                  "label": "16,6%"
               }
            ]
         },
         {
            "type": "select",
            "id": "col_pr_mb",
            "label": "Banner width (mobile)",
            "default": "12",
            "options": [
               {
                  "value": "12",
                  "label": "100%"
               },
               {
                  "value": "6",
                  "label": "50%"
               }
            ]
         },
         {
            "type": "header",
            "content": "== Content settings"
         },
         {
            "type": "range",
            "id": "cap_v_pos",
            "label": "Content vertical position",
            "info":" <= 50: Top position , > 50 bottom position",
            "default": 50,
