            "label": "Content horizontal position",
            "info":" <= 50: Left position , > 50 right position",
            "default": 50,
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "%"
         },
         {
            "type": "select",
            "id": "txt_align_mb",
            "label": "Text alignment",
            "default": "tc",
            "options": [
               {
                  "value": "tl",
                  "label": "Left"
               },
               {
                  "value": "tc",
                  "label": "Center"
               },
               {
                  "value": "tr",
                  "label": "Right"
               }
            ]
         },
         {
            "type": "header",
            "content": "== Color settings"
         },
         {
            "type": "color",
            "id": "cl_overlay",
            "label": "Overlay",
            "default": "#000"
         },
         {
            "type": "range",
            "id": "cl_opacity",
            "label": "Overlay opacity",
            "default": 0,
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "%"
         },
         {
            "type": "header",
            "content": "== Background Color content settings"
         },
         {
            "type": "checkbox",
            "id": "bg_content_bl",
            "label": "Enable Background content Color?",
            "default": false
         },
         {
            "type": "color",
            "id": "bg_content_cl",
            "label": "Background Color",
            "default": "#fff"
         },
         {
            "type": "range",
            "id": "bg_content_op",
            "label": "Background Color opacity",
            "default": 50,
            "min": 0,
            "max": 100,
            "step": 1,
            "unit": "%"
         },
         {
            "type": "text",
            "id": "content_pd",
            "label": "Content pading (desktop)",
            "default": "40px 80px"
         },
         {
            "type": "text",
            "id": "content_pd_mb",
            "label": "Content pading (mobile)",
            "default": "10px 20px"
         },
         {"type": "paragraph","content": "————————————————"},
         {
            "type": "checkbox",
            "id": "border_bl",
            "label": "Enable Border content Color?",
            "default": false
         },
         {
            "type": "color",
            "id": "br_color",
            "label": "Border Color",
            "default": "#222"
         },
