<script src="https://cdn.tailwindcss.com"></script>
<script>
  tailwind.config = {
    theme: {
      extend: {
        colors: {
          clifford: '#da373d',
        }
      }
    },
    important: '#magazine',
  }
</script>

{%- liquid 
  assign admin_sp = request.design_mode
  assign sid = section.id
  assign se_stts = section.settings
  assign se_blocks = section.blocks
  assign font_family = settings.fnt_magazine.family | default: settings.fnt_magazine2.family
  assign DefaultValueMrLR = 'auto'
  assign DefaultValueMr = 0 
  assign DefaultValuePd = 0
  assign fs_text = se_stts.fs_text
  assign fs_text_m = se_stts.fs_text_m -%}

{% for block in se_blocks %}
  {%- liquid
    assign bk_stts = block.settings
    assign mg = bk_stts.mg
    assign pd = bk_stts.pd
    assign mg_mb = bk_stts.mg_mb
    assign pd_mb = bk_stts.pd_mb -%}

  {%- capture block_style -%}
    {%- if mg != blank or pd != blank -%} 
    .page_{{ block.type }} .mag_container {
      {%- if mg != blank %}
        {% assign arr_mg = mg | remove:' ' | split:',' -%}
        margin-top: {{arr_mg[0]|default:DefaultValueMr}} !important;
        margin-right: {{arr_mg[1]|default:DefaultValueMrLR}} !important;
        margin-bottom: {{arr_mg[2]|default:DefaultValueMr}} !important;
        margin-left: {{arr_mg[3]|default:DefaultValueMrLR}} !important;
      {%- endif -%}
      {%- if pd != blank %}
        {% assign arr_pd = pd | remove:' ' | split:',' -%}
          padding-top: {{arr_pd[0]|default:DefaultValuePd}} !important;
          padding-right: {{arr_pd[1]|default:DefaultValuePd}} !important;
          padding-bottom: {{arr_pd[2]|default:DefaultValuePd}} !important;
          padding-left: {{arr_pd[3]|default:DefaultValuePd}} !important;
      {%- endif -%}
    }
    {%- endif -%}
    {%- if mg_mb != blank or pd_mb != blank -%} 
      @media only screen and (max-width: 1279px) {
        .page_{{ block.type }} .mag_container {
          {%- if mg_mb != blank %}
            {% assign arr_mg = mg_mb | remove:' ' | split:',' -%}
            margin-top: {{arr_mg[0]|default:DefaultValueMr}} !important;
            margin-right: {{arr_mg[1]|default:DefaultValueMrLR}} !important;
            margin-bottom: {{arr_mg[2]|default:DefaultValueMr}} !important;
            margin-left: {{arr_mg[3]|default:DefaultValueMrLR}} !important;
          {%- endif -%}
          {%- if pd_mb != blank %}
            {% assign arr_pd = pd_mb | remove:' ' | split:',' -%}
              padding-top: {{arr_pd[0]|default:DefaultValuePd}} !important;
              padding-right: {{arr_pd[1]|default:DefaultValuePd}} !important;
              padding-bottom: {{arr_pd[2]|default:DefaultValuePd}} !important;
              padding-left: {{arr_pd[3]|default:DefaultValuePd}} !important;
          {%- endif -%}
        }
      }
    {%- endif -%}
  {%- endcapture -%}

  {%- style -%}{{ block_style | strip_newlines | remove:"  "}}{%- endstyle -%}

{% endfor %}

<div class="nt_se_{{sid}} {{se_stts.layout}}" id="magazine">
  <a href="/pages/magazine" >
    <div class="backToCatalogPage w-max mb-10 p-2 border border-solid border-gray-500 rounded hover:border-black text-gray-500 hover:text-black md:cursor-pointer">
      ALL MAGAZINE
    </div>
  </a>
  <div class="magazine_wrapper relative">
    <div class="append-buttons flex justify-between px-items-center mb-5 text-base font-normal">
      <div class="append-buttons-item prevSlide md:cursor-pointer flex items-center">
        <i class="iconfont icon-pex-xiangzuo"></i> PREVIOUS PAGE
      </div>
      <div class="flex">
        <div class="append-buttons-item nextSlide mr-4 md:cursor-pointer flex items-center">
          NEXT PAGE <i class="iconfont icon-pex-xiangyou1"></i>
        </div>
        <div class="append-buttons-item backToFirstSlide md:cursor-pointer flex items-center">
          BACK<i class="iconfont icon-pex-xiangyou"></i>
        </div>
      </div>
    </div>
    <div class="swiper mySwiper border border-solid border-[#F0F2F5] relative">
      <div class="swiper-wrapper">
        {% for block in section.blocks %}
          {%- assign bk_stts = block.settings -%}
          {%- assign arr_pd = bk_stts.pd | remove:' ' | split:',' -%}
          {%- assign product = bk_stts.product -%}
          {%- assign product2 = bk_stts.product2 -%}
          {% assign arr_pd_m = pd_mb | remove:' ' | split:',' -%}

          {%- capture page_num_r -%}
            <div class="page_number absolute xl:right-[{{ arr_pd[1]|default:DefaultValuePd }}] right-[{{ arr_pd_m[1]|default:DefaultValuePd }}] xl:bottom-[5%] bottom-[2.5%] flex items-baseline">
              <div class="text-xs">{{ bk_stts.page_text }}</div>
              <div class="ml-1 text-lg font-bold">{{ bk_stts.page_number }}</div>
            </div>
          {%- endcapture -%}

          {%- capture page_num_l -%}
            <div class="page_number absolute xl:left-[{{ arr_pd[3]|default:DefaultValuePd }}] left-[{{ arr_pd_m[3]|default:DefaultValuePd }}] xl:bottom-[5%] bottom-[2.5%] flex items-baseline">
              <div class="text-xs">{{ bk_stts.page_text }}</div>
              <div class="ml-1 text-lg font-bold">{{ bk_stts.page_number }}</div>
            </div>
          {%- endcapture -%}

          <div class="swiper-slide page_{{ block.type }}" key="{{ block.type }}">
          {%- case block.type -%}
            {%- when 'magazine_template_1' -%}
              <div class="flex flex-col justify-between h-full">
                <div class="mag_container">
                  <div class="mag_header mb-5 font-bold text-black">
                    <div class="mag_sub_heading xl:text-xl text-[1.2rem]">{{ bk_stts.sub_heading | escape }}</div>
                    <div class="mag_heading xl:text-6xl text-[2.8rem]">{{ bk_stts.heading | escape }}</div>
                    <div class="mag_third_heading xl:text-lg text-[1.1rem] leading-none">{{ bk_stts.third_heading }}</div>
                  </div>
                  <div class="mag_content xl:mb-7 mb-5 grid grid-cols-2 gap-7 font-normal text-black">
                    <div class="indent-4">
                      {{ bk_stts.richtext1 }}
                    </div>
                    <div>
                      {{ bk_stts.richtext2 }}
                    </div>
                  </div>
                </div>

                <div class="mag_image">
                  <img src="{{ bk_stts.image | img_url: "master" }}" alt="{{bk_stts.image.alt}}" />
                </div>
              </div>

            {%- when 'magazine_template_2' -%}
              <div class="flex flex-col justify-between h-full">
                <div class="mag_text mag_container">
                  <div class="mag_content xl:mb-7 mb-5 grid grid-cols-2 xl:gap-7 gap-5 font-normal text-black">
                    <div>
                      {{ bk_stts.richtext1 }}
                    </div>
                    <div>
                      {{ bk_stts.richtext2 }}
                    </div>
                  </div>
                </div>
                
                <div class="mag_image">
                  <img src="{{ bk_stts.image | img_url: "master" }}" alt="{{bk_stts.image.alt}}" />
                </div>
              </div>

            {%- when 'magazine_template_3' -%}
              <div class="page_wrapper h-full">
                <a class="h-full" href="{{ bk_stts.url | default: product.url }}" class="mag_image block">
                  <img class="h-full" src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt | default: product.title }}" />
                  <span class="sr-only">{{ bk_stts.image.alt | default: product.title }}</span>
                </a>
              </div>

            {%- when 'magazine_template_4' -%}
              <div class="page_wrapper relative flex justify-between h-full">
                <div class="mag_image col-4 h-full p-0">
                  <img class="h-full" src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt }}" />
                  <span class="sr-only">{{ bk_stts.image.alt }}</span>
                </div>

                <div class="mag_text mag_container flex-1 self-center">
                  <div class="mag_heading md:mb-7 mb-5 xl:text-5xl text-[2.4rem] font-bold">
                    <span class="relative">
                      {{ bk_stts.heading }}
                      <img class="absolute right-0 -top-[25px]" src="{{ bk_stts.tag_image | img_url: "master" }}" alt="{{ bk_stts.tag_image.alt }}" />
                    </span>
                  </div>
                  <div class="mag_content">{{ bk_stts.richtext }}</div>
                </div>

                {{ page_num_r }}
              </div>
            
            {%- when 'magazine_template_5' -%}
              <div class="page_wrapper mag_container h-full">
                <div class="mag_text flex justify-between items-center h-full">
                  <div class="col-8 flex flex-col xl:p-[30px] p-[15px]">
                    <div class="mag_heading xl:mb-7 mb-5 font-bold">
                      <div class="xl:text-3xl text-[1.8rem] leading-none">
                        {{ bk_stts.sub_heading }}
                      </div>
                      <span class="xl:text-5xl text-[2.4rem]">
                        {{ bk_stts.heading }}
                      </span>
                    </div>
                    <div class="mag_content">{{ bk_stts.richtext }}</div>
                  </div>

                  {%- if bk_stts.image != blank -%}
                    <div class="mag_image flex justify-end h-full col-4 xl:pb-0 px-0 pb-[12px]">
                      <img class="h-full" src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt }}" />
                      <span class="sr-only">{{ bk_stts.image.alt }}</span>
                    </div>
                  {%- endif -%}
                </div>
              </div>

            {%- when 'magazine_template_6' -%}
              <div class="page_wrapper mag_container relative h-full">
                <div class="mag_text h-full">
                  {%- if bk_stts.image != blank -%}
                    <div class="mag_image h-full">
                      <img class="xl:h-full h-auto" src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt }}" />
                      <span class="sr-only">{{ bk_stts.image.alt }}</span>
                    </div>
                  {%- endif -%}
                </div>

                {{ page_num_r }}
              </div>

            {%- when 'magazine_template_7' -%}
              <div class="page_wrapper mag_container h-full flex flex-col">
                <div class="flex justify-between items-center">
                  {%- if bk_stts.image != blank -%}
                    <a href="{{ bk_stts.url | default: product.url }}" class="mag_image block col-8">
                      <img src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt }}" />
                      <span class="sr-only">{{ bk_stts.image.alt }}</span>
                      <div class="mt-2 text-center text-sm text-[#666666] hover:text-black hover:underline lg:no-underline underline">{{ product.title }}</div>
                    </a>
                  {%- endif -%}

                  <div class="flex flex-col col-4">
                    {% for image in product.images limit: 3 %}
                      <view class="relative w-full pt-[100%]">
                        <img class="w-full h-full top-0 left-0 absolute" src="{{ image.src | product_img_url: 'medium' }}" alt="{{ product.title }}" mode="aspectFill" />
                      </view>
                    {% endfor %}
                  </div>
                </div>
                <div class="mag_text_wrapper 2xl:mt-7 mt-4">
                  <div class="mag_heading 2xl:text-5xl text-[2.4rem] font-bold text-[{{ bk_stts.heading_color }}]">{{ bk_stts.heading }}</div>
                  <div class="mag_content">{{ bk_stts.content }}</div>
                </div>
              </div>

            {%- when 'magazine_template_8' -%}
              <div class="page_wrapper mag_container relative h-full flex flex-col">
                <div class="mag_text_wrapper">
                  <div class="mag_heading 2xl:text-5xl text-[2.4rem] font-bold text-[{{ bk_stts.heading_color }}]">{{ bk_stts.heading }}</div>
                  <div class="mag_content">{{ bk_stts.content }}</div>
                </div>

                <div class="flex justify-between items-center 2xl:mt-7 mt-4">
                  {%- if bk_stts.image != blank -%}
                    <a href="{{ bk_stts.url | default: product.url }}" class="mag_image block 2xl:flex-[0_0_66.66667%] 2xl:max-w-[66.66667%] flex-[0_0_58.333333%] max-w-[58.333333%]">
                      <img src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt }}" />
                      <span class="sr-only">{{ bk_stts.image.alt }}</span>
                      <div class="mt-2 text-center text-[#666666] hover:text-black hover:underline lg:no-underline underline">{{ product.title }}</div>
                    </a>
                  {%- endif -%}

                  <div class="flex flex-col flex-[0_0_41.666667%] max-w-[41.666667%] 2xl:flex-[0_0_33.333333%] 2xl:max-w-[33.333333%]">
                    {% for image in product.images limit: 2 %}
                      <view class="relative w-full pt-[100%]">
                        <img class="w-full h-full top-0 left-0 absolute" src="{{ image.src | product_img_url: 'medium' }}" alt="{{ product.title }}" mode="aspectFill" />
                      </view>
                    {% endfor %}
                  </div>
                </div>

                {{ page_num_r }}
              </div>

            {%- when 'magazine_template_9' -%}
              <div class="page_wrapper relative h-full">
                <div class="mag_content absolute top-[5%] left-[5%]">
                  <a class="block underline text-[#666666] hover:text-black" href="{{ bk_stts.url | default: product.url }}">{{ bk_stts.text | default: product.title }}</a>
                  <a class="block underline text-[#666666] hover:text-black" href="{{ bk_stts.url2 | default: product2.url }}">{{ bk_stts.text2 | default: product2.title }}</a>
                </div>

                <a href="{{ bk_stts.url | default: product.url }}" class="mag_image block">
                  <img src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt | default: product.title  }}" />
                  <span class="sr-only">{{ bk_stts.image.alt | default: product.title }}</span>
                </a>
              </div>

            {%- when 'magazine_template_10' -%}
              <div class="page_wrapper h-full">
                <a href="{{ bk_stts.url | default: product.url }}" class="mag_image block">
                  <img src="{{ bk_stts.image | img_url: "master" }}" alt="{{ bk_stts.image.alt | default: product.title }}" />
                  <span class="sr-only">{{ bk_stts.image.alt | default: product.title }}</span>
                </a>
              </div>

            {%- when 'magazine_template_11' -%}
              <div class="page_wrapper mag_container flex justify-center items-center relative h-full">
                <div class="grid grid-cols-2 px-[5%]">
                  <div class="magazine12_item_content relative h-max xl:px-[10%] px-[5%] xl:pb-[10%] pb-[5%]">
                    <a class="flex justify-center" href="{{ bk_stts.url | default: product.url }}">
                      <img src="{{ bk_stts.product_image1 | img_url: "master" }}" />
                    </a>
                    <div class="mt__5">
                      <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url | default: product.url }}">{{ product.title }}</a>
                      <div class="xl:mt-4 mt-2 text-3xl font-bold">{{ bk_stts.title1 }}</div>
                      <div class="text_content xl:mt-4 mt-2">{{ bk_stts.content1 }}</div>
                    </div>
                  </div>
                  <div class="xl:mt-24 mt-12 xl:px-[10%] px-[5%]">
                    <div class="xl:mb-5 mb-3">
                      <div class="text-3xl font-bold">{{ bk_stts.title2 }}</div>
                      <div class="text_content xl:mt-4 mt-2">{{ bk_stts.content2 }}</div>
                    </div>
                    <div class="flex flex-col">
                      <a href="{{ bk_stts.url2 | default: product2.url }}">
                        <img src="{{ bk_stts.product_image2 | img_url: "master" }}" alt="{{ product2.title }}" />
                      </a>
                      <a class="mt__5 text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url2 | default: product2.url }}">{{ product2.title }}</a>
                    </div>
                  </div>
                </div>

                <div class="absolute right-0 bottom-0 h-full">
                  <img class="h-full" src="{{ bk_stts.right_image | img_url: 'master' }}" alt="{{ bk_stts.right_image.alt | default: product.title }}"/>
                </div>
                <div class="absolute left-0 bottom-0 xl:w-[65%] w-[55%]">
                  <img src="{{ bk_stts.bottom_image | img_url: 'master' }}" alt="{{ bk_stts.bottom_image.alt | default: product2.title }}"/>
                </div>
              </div>

            {%- when 'magazine_template_12' -%}
              <div class="page_wrapper mag_container flex justify-center items-center relative h-full">
                <div class="grid grid-cols-2 px-[5%]">
                  <div class="magazine12_item_content relative h-max xl:px-[10%] px-[5%] xl:pb-[10%] pb-[5%]">
                    <a class="flex justify-center" href="{{ bk_stts.url | default: product.url }}">
                      <img src="{{ bk_stts.product_image1 | img_url: "master" }}" />
                    </a>
                    <div class="mt__5">
                      <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url | default: product.url }}">{{ product.title }}</a>
                      <div class="xl:mt-4 mt-2 text-3xl font-bold">{{ bk_stts.title1 }}</div>
                      <div class="xl:mt-4 mt-2 text_content">{{ bk_stts.content1 }}</div>
                    </div>
                  </div>
                  <div class="xl:mt-24 mt-12 xl:px-[10%] px-[5%]">
                    <div class="xl:mb-5 mb-3">
                      <div class="text-3xl font-bold">{{ bk_stts.title2 }}</div>
                      <div class="xl:mt-4 mt-2 text_content">{{ bk_stts.content2 }}</div>
                    </div>
                    <div class="flex flex-col">
                      <a href="{{ bk_stts.url2 | default: product2.url }}">
                        <img src="{{ bk_stts.product_image2 | img_url: "master" }}" alt="{{ product2.title }}" />
                      </a>
                      <a class="mt__5 text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url2 | default: product2.url }}">{{ product2.title }}</a>
                    </div>
                  </div>
                </div>

                <div class="absolute top-0 left-0  h-full">
                  <img class="h-full" src="{{ bk_stts.left_image | img_url: 'master' }}" alt="{{ bk_stts.left_image.alt | default: product.title }}"/>
                </div>
                <div class="absolute top-0 right-0 xl:w-[55%] w-[40%]">
                  <img class="md:w-full w-[70%] float-right" src="{{ bk_stts.top_image | img_url: 'master' }}" alt="{{ bk_stts.top_image.alt | default: product2.title }}"/>
                </div>
              </div>

            {%- when 'magazine_template_13' -%}
              <div class="page_wrapper mag_container h-full">
                <div class="mag_img w-2/3">
                  <img src="{{ bk_stts.header_image | img_url: "master" }}" alt="{{ bk_stts.header_image.alt }}" />
                </div>
                <div class="mag_content">{{ bk_stts.content }}</div>
                <div class="flex justify-between xl:mt-10 mt-5">
                  <div class="relative col-6 xl:mt-10 mt-5">
                    <a class="flex justify-center" href="{{ bk_stts.url | default: product.url }}">
                      <img src="{{ bk_stts.image1 | img_url: "master" }}" alt="{{ bk_stts.image1.alt }}" />
                    </a>
                    <div>
                      <div class="text-xl font-bold">{{ bk_stts.title1 }}</div>
                      <div class="text_content">
                        <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url | default: product.url }}">{{ bk_stts.content1 | default: product.title }}</a>
                      </div>
                    </div>
                  </div>
                  <div class="relative col-6">
                    <a class="flex justify-center" href="{{ bk_stts.url | default: product2.url }}">
                      <img src="{{ bk_stts.image2 | img_url: "master" }}" alt="{{ bk_stts.image2.alt }}" />
                    </a>
                    <div>
                      <div class="text-xl font-bold">{{ bk_stts.title2 }}</div>
                      <div class="text_content">
                        <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url2 | default: product2.url }}">{{ bk_stts.content2 | default: product2.title }}</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

            {%- when 'magazine_template_14' -%}
              <div class="page_wrapper mag_container relative flex justify-between items-center h-full">
                <div class="flex flex-col col-6 xl:mt-10 mt-5">
                  <div class="relative">
                    <a class="flex justify-center" href="{{ bk_stts.url1 | default: product.url }}">
                      <img src="{{ bk_stts.image1 | img_url: "master" }}" alt="{{ bk_stts.image1.alt }}" />
                    </a>
                    <div class="w-4/5">
                      <div class="text-xl font-bold">{{ bk_stts.title1 }}</div>
                      <div class="text_content">
                        <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url1 | default: product.url }}">{{ bk_stts.content1 }}</a>
                      </div>
                    </div>
                  </div>
                  <div class="relative xl:mt-5 mt-3">
                    <a class="flex justify-center" href="{{ bk_stts.url12 | default: product2.url }}">
                      <img src="{{ bk_stts.image2 | img_url: "master" }}" alt="{{ bk_stts.image2.alt }}" />
                    </a>
                    <div class="w-4/5">
                      <div class="text-xl font-bold">{{ bk_stts.title2 }}</div>
                      <div class="text_content">
                        <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url12 | default: product2.url }}">{{ bk_stts.content2 }}</a>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="flex flex-col col-6">
                  <div class="relative">
                    <a class="flex justify-center" href="{{ bk_stts.url3 | default: product3.url }}">
                      <img src="{{ bk_stts.image3 | img_url: "master" }}" alt="{{ bk_stts.image3.alt }}" />
                    </a>
                    <div class="w-4/5">
                      <div class="text-xl font-bold">{{ bk_stts.title3 }}</div>
                      <div class="text_content">
                        <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url3 | default: product3.url }}">{{ bk_stts.content3 }}</a>
                      </div>
                    </div>
                  </div>
                  <div class="relative xl:mt-5 mt-3">
                    <a class="flex justify-center" href="{{ bk_stts.url4 | default: product4.url }}">
                      <img src="{{ bk_stts.image4 | img_url: "master" }}" alt="{{ bk_stts.image4.alt }}" />
                    </a>
                    <div class="w-4/5">
                      <div class="text-xl font-bold">{{ bk_stts.title4 }}</div>
                      <div class="text_content">
                        <a class="text-[#666666] hover:text-black hover:underline lg:no-underline underline text-sm" href="{{ bk_stts.url4 | default: product4.url }}">{{ bk_stts.content4 }}</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

