{%- style -%}
.nt_se_{{sid}} {
  font-family: {{ font_family | default:font_family2 }};
}
.nt_se_{{sid}} .swiper-slide {
  font-size: {{ fs_text }}px;
}
@media screen and (min-width: 1025px) and (max-width: 1440px) {
  .nt_se_{{sid}} .swiper-slide {
    font-size: 14px;
  }
}
@media screen and (max-width: 767px) {
  .nt_se_{{sid}} .swiper-slide {
    font-size: {{ fs_text_m }}px;
  }
}
.magazine12_item_content:before {
  content: "";
  position: absolute;
  bottom: 0%;
  left: 0;
  width: 100%;
  height: 75%;
  z-index: -1;
  background-color: rgba(242, 242, 242, 1);
  -webkit-clip-path: polygon(100% 0%,100% 100%,100% 100%,0 100%,0 15%,25% 0%);
  clip-path: polygon(100% 0%,100% 100%,100% 100%,0 100%,0 15%,25% 0%);
}
.qs_title:before {
  content: "";
  position: absolute;
  left: 0;
  width: 7px;
  height: 100%;
}
.swiper {
	--swiper-navigation-color: #999;
	--swiper-pagination-color: #999;
  --swiper-navigation-size: 30px;
}
{%- endstyle -%}

